{
    "name": "withcandour/statamic-toasty",
    "description": "Automatic static cache warming for Statamic",
    "version": "1.0.0",
    "type": "statamic-addon",
    "autoload": {
        "psr-4": {
            "WithCandour\\StatamicToasty\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Candour",
            "email": "devs@withcandour.co.uk"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Statamic Toasty",
            "description": "Automatic static cache warming for Statamic."
        },
        "laravel": {
            "providers": [
                "WithCandour\\StatamicToasty\\ToastyServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^5",
        "symfony/browser-kit": "^7",
        "symfony/http-client": "^7"
    }
}
